<odoo>    <record id="view_country_nationality_form" model="ir.ui.view">        <field name="name">res.country.nationality</field>        <field name="model">res.country</field>        <field name="inherit_id" ref="base.view_country_form"/>        <field name="arch" type="xml">            <xpath expr="//field[@name='vat_label']" position="after">                <field name="nationality"/>            </xpath>        </field>    </record>    <record id="view_country_tree" model="ir.ui.view">        <field name="name">res.country.nationality</field>        <field name="model">res.country</field>        <field name="inherit_id" ref="base.view_country_tree"/>        <field name="arch" type="xml">            <xpath expr="//field[@name='code']" position="after">                <field name="nationality"/>            </xpath>        </field>    </record></odoo>